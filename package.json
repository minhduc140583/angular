{
  "name": "angular-example-app",
  "version": "6.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve --open",
    "http-server": "http-server dist/ -c-1",
    "build": "ng build",
    "build:library": "ng build ngx-example-library",
    "deploy": "ng build --prod && angular-cli-ghpages",
    "test": "ng test --watch",
    "test-auto": "wdio ./wdio.conf.js",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage",
    "test:library": "ng test ngx-example-library",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:library": "ng lint ngx-example-library",
    "ci": "npm run lint && npm run lint:library && npm run test && npm run test:library && npm run e2e",
    "sme": "ng build && source-map-explorer dist/main.bundle.js",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "docker": "docker build -t angularexampleapp . && docker run -d -p 4200:80 angularexampleapp"
  },
  "private": true,
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.0.0"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.801.3",
    "@angular/animations": "8.1.3",
    "@angular/common": "^8.1.3",
    "@angular/core": "^8.1.3",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "^8.1.3",
    "@angular/platform-browser": "8.1.3",
    "@angular/platform-browser-dynamic": "8.1.3",
    "@angular/router": "^8.1.3",
    "@asymmetrik/ngx-leaflet": "^6.0.1",
    "angularx": "0.0.5",
    "authentication-component": "0.0.5",
    "classlist.js": "1.1.20150312",
    "core-component": "0.0.7",
    "core-js": "3.1.4",
    "csvtojson": "^2.0.8",
    "datepicker": "0.0.0",
    "form-util": "0.0.5",
    "leaflet": "^1.5.1",
    "locale-service": "0.0.4",
    "metadata-x": "0.0.4",
    "model-formatter": "0.0.4",
    "ng-pick-datetime": "^7.0.0",
    "ngx-bootstrap": "~5.1.0",
    "ngx-cookie-service": "^2.2.0",
    "ngx-pagination": "^4.1.0",
    "onecore": "0.0.8",
    "password-component": "0.0.7",
    "phonecodes": "0.0.1",
    "reflectx": "0.0.9",
    "rxjs": "^6.5.2",
    "search-utilities": "0.0.5",
    "timepicker": "^1.11.15",
    "ui-alert": "0.0.5",
    "ui-loading": "0.0.4",
    "ui-plus": "0.0.4",
    "ui-toast": "0.0.4",
    "validation-util": "0.0.3",
    "web-clients": "^0.1.6",
    "zone.js": "^0.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-ng-packagr": "~0.801.3",
    "@angular/cdk": "^8.1.2",
    "@angular/cli": "^8.1.3",
    "@angular/compiler": "^8.1.3",
    "@angular/compiler-cli": "^8.1.3",
    "@angular/language-service": "8.1.3",
    "@angular/material": "^8.1.2",
    "@angular/platform-server": "8.1.3",
    "@angular/service-worker": "8.1.3",
    "@asymmetrik/ngx-leaflet": "^6.0.1",
    "@ismaestro/ngx-example-library": "^1.0.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/html2canvas": "0.0.35",
    "@types/jasmine": "3.3.16",
    "@types/jasminewd2": "2.0.6",
    "@types/leaflet": "^1.5.0",
    "@types/node": "12.6.8",
    "angular-cli-ghpages": "0.5.3",
    "babel-core": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "5.1.0",
    "coveralls": "3.0.5",
    "hammerjs": "2.0.8",
    "html2canvas": "1.0.0-rc.3",
    "http-server": "0.11.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.0.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-remap-istanbul": "0.6.0",
    "karma-scss-preprocessor": "4.0.0",
    "leaflet": "^1.5.1",
    "mocha": "^6.2.0",
    "moment": "^2.24.0",
    "ng-packagr": "^5.4.3",
    "node-sass": "^4.9.0",
    "protractor": "5.4.2",
    "source-map-explorer": "2.0.1",
    "standard-version": "8.0.2",
    "ts-helpers": "1.1.2",
    "ts-node": "8.3.0",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "5.18.0",
    "typescript": "~3.4.5",
    "wdio-junit-reporter": "^0.4.4",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-selenium-standalone-service": "0.0.12",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^5.11.11",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/.{js,jsx,mjs}",
      "<rootDir>/src/**/?(.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "node",
      "mjs"
    ]
  }
}
